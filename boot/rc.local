#!/bin/bash
#
# rc.local
#
# This script is executed at the end of each multiuser runlevel.
# Make sure that the script will "exit 0" on success or any other
# value on error.
#
# In order to enable or disable this script just change the execution
# bits.
#
# By default this script does nothing.
sleep 1
su -c "/home/ubuntu/projects/newscollection/update_model.sh" -s /bin/bash ubuntu
su -c "/home/ubuntu/projects/newscollection/update_boost.sh" -s /bin/bash ubuntu
su -c "/home/ubuntu/projects/newscollection/pack_new_model.sh" -s /bin/bash ubuntu
su -c "/home/ubuntu/projects/newscollection/deploy_new_model.sh" -s /bin/bash ubuntu
exit 0
